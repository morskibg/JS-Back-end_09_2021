{
	"extends": ["airbnb","plugin:node/recommended"],
	
	"rules": {
		
		"no-unused-vars":"warn",
		"no-console":"off",
		"func-names":"off",
		"class-methods-use-this":"off",
		"no-process-exit":"off",
		"comma-dangle": ["error", "always-multiline"]	,
		"no-tabs": ["error", { "allowIndentationTabs": true }],
		"indent": ["error", "tab"],
		"no-underscore-dangle": ["error", { "allow": ["_id"] }]
	}
}
